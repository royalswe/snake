{
	"name": "snake",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"start:dev": "nodemon",
		"build": "vite build",
		"move": "rm -rf ../../mongot_prod/snake.mongot.com/client && mv build ../../mongot_prod/snake.mongot.com/client",
		"preview": "vite preview",
