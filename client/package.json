		"@typescript-eslint/parser": "^5.47.0",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss-load-config": "^4.0.1",
		"postcss-preset-env": "^7.8.3",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.0",
		"svelte-check": "^2.10.3",
		"svelte-preprocess": "^5.0.0",
