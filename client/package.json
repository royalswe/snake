{
	"name": "snake",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --https",
		"start:dev": "nodemon",
		"build": "vite build",
		"move": "rm -rf ../../mongot_prod/snake.mongot.com/client && mv build ../../mongot_prod/snake.mongot.com/client",
		"move-build": "vite build && rm -rf ../../mongot_prod/snake.mongot.com/client && mv build ../../mongot_prod/snake.mongot.com/client",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.1",
		"@sveltejs/kit": "^1.13.0",
		"@types/postcss-preset-env": "^8.0.0",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"postcss-preset-env": "^8.0.1",
		"prettier": "^2.8.6",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.57.0",
		"svelte-check": "^3.1.4",
		"svelte-preprocess": "^5.0.3",
		"tslib": "^2.5.0",
		"typescript": "^5.0.2",
		"vite": "^4.2.1",
		"vite-plugin-mkcert": "^1.13.3"
	}
}
