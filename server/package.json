{
  "name": "snake-server",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run build && node dist/app.js",
    "build": "rimraf ./dist && tsc -p tsconfig.prod.json",
    "move": "rm -rf ./dist/models && rm -rf ../../mongot_prod/snake.mongot.com/server && mv dist ../../mongot_prod/snake.mongot.com/server",
    "build-move": "npm run build && rm -rf ./dist/models && rm -rf ../../mongot_prod/snake.mongot.com/server && mv dist ../../mongot_prod/snake.mongot.com/server",
    "dev": "ALIEN_UWS=1 NODE_ENV=development NODE_OPTIONS='--inspect' ts-node-dev --respawn src/app.ts",
    "dev:con": "nodemon --exec ts-node src/app.ts",
    "test1": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest .",
    "test-native": "node --test test/*"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/client-sessions": "^0.8.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.3.1",
    "@types/uws": "^0.13.3",
    "jest": "^29.5.0",
    "node-dev": "^8.0.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "client-sessions": "^0.8.0",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v20.30.0"
  }
}
