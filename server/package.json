{
  "name": "snake-server",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run build && node dist/app.js",
    "build": "rimraf ./dist && tsc",
    "move": "npm run move && rm -rf ../../mongot_prod/snake.mongot.com/server && mv dist ../../mongot_prod/snake.mongot.com/server",
    "dev": "NODE_ENV=development ts-node-dev --respawn src/app.ts",
    "dev:con": "nodemon --exec ts-node src/app.ts"
  },
  "devDependencies": {
    "@types/node": "^18.11.7",
    "@types/uws": "^0.13.3",
    "node-dev": "^7.4.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "client-sessions": "^0.8.0",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v20.15.0"
  }
}
